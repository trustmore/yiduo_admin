{
    "presets": ["latest", "react", "stage-0"],
    "plugins": [
        ["import", { "libraryName": "antd", "style": "true" }],
        "transform-runtime",
        "add-module-exports",
        "transform-class-properties",
        "transform-react-display-name",
        "transform-decorators-legacy"
    ]
}
